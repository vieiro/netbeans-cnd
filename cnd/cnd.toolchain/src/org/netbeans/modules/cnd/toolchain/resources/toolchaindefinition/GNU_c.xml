<toolchaindefinition xmlns="http://www.netbeans.org/ns/cnd-toolchain-definition/1">
    <c>
        <compiler name="gcc"/>
        <version flags="--version"/>
        <system_include_paths flags="-x c -E -v" parser="GNU"/>
        <important_flags flags="-O(\W|$|-)|-O1|-O2|-O3|-O4|-O5|-Ofast|-Og|-Os|-ansi|-fPIC|-fPIE|-fasynchronous-unwind-tables|-fbuilding-libgcc|-fcilkplus|-fcx-fortran-rules|-fcx-limited-range|-fexceptions|-ffast-math|-ffinite-math-only|-ffreestanding|-fgnu-tm|-fgnu89-inline|-fhandle-exceptions|-fleading-underscore|-fno-exceptions|-fno-rtti|-fnon-call-exceptions|-fopenacc|-fopenmp|-fpic|-fpie|-freciprocal-math|-fsanitize=.*|-fshort-double|-fshort-wchar|-fsignaling-nans|-fsingle-precision-constant|-fstack-protector(\W|$|-)|-fstack-protector-all|-fstack-protector-strong|-funsafe-math-optimizations|-funsigned-char|-funwind-tables|-g(\W|$|-)|-ggdb|-gsplit-dwarf|-gtoggle|-isysroot|-m128bit-long-double|-m16|-m32|-m3dnow|-m64|-m96bit-long-double|-mabm|-madx|-maes|-mandroid|-march=.*|-mavx(\W|$|-)|-mavx2|-mavx512bw|-mavx512cd|-mavx512dq|-mavx512er|-mavx512f|-mavx512ifma|-mavx512pf|-mavx512vbmi|-mavx512vl|-mbionic|-mbmi(\W|$|-)|-mbmi2|-mclflushopt|-mclwb|-mcx16|-mf16c|-mfma(\W|$|-)|-mfma4|-mfsgsbase|-mlong-double-128|-mlong-double-64|-mlwp|-mlzcnt|-mmpx|-mmwaitx|-mpclmul|-mpcommit|-mpopcnt|-mprefetchwt1|-mprfchw|-mrdrnd|-mrdseed|-mrtm|-msha|-msoft-float|-msse3|-msse4(\W|$|-)|-msse4.1|-msse4.2|-msse4a|-msse5|-mssse3|-mtbm|-mtune=.*|-muclibc|-mx32|-mxop|-mxsave(\W|$|-)|-mxsavec|-mxsaveopt|-mxsaves|-nostdlib|-pthreads|-std=.*|-xc($|\+\+$)"/>
        <system_macros flags="-x c -E -dM" parser="GNU">
            <macro stringvalue="__STDC__=1"/>
        </system_macros>
        <user_include flags="-I"/>
        <user_file flags="-include "/>
        <user_macro flags="-D"/>
        <development_mode>
            <fast_build flags=""/>
            <debug flags="-g" default="true"/>
            <performance_debug flags="-g -O"/>
            <test_coverage flags="-g"/>
            <diagnosable_release flags="-g -O2"/>
            <release flags="-O2"/>
            <performance_release flags="-O3"/>
        </development_mode>
        <warning_level>
            <no_warnings flags="-w"/>
            <default flags="" default="true"/>
            <more_warnings flags="-Wall"/>
            <warning2error flags="-Werror"/>
        </warning_level>
        <architecture>
            <default flags="" default="true"/>
            <bits_32 flags="-m32"/>
            <bits_64 flags="-m64"/>
        </architecture>
        <strip flags="-s"/>
        <c_standard>
            <default flags="" default="true"/>
            <c89 flags="-std=c89"/>
            <c99 flags="-std=c99"/>
            <c11 flags="-std=c11"/>
        </c_standard>
        <output_object_file flags="-o " />
        <dependency_generation flags="-MMD -MP -MF &quot;$@.d&quot;" />
        <precompiled_header flags="-o &quot;$@&quot;" suffix=".gch" append="true" />
    </c>
</toolchaindefinition>
